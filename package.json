{
  "name": "ts-node-template",
  "version": "1.0.0",
  "description": "tamplate for NodeJs",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc", 
    "dev": "concurrently \"npm run build:watch\" \"npm run start:watch\"", 
    "start": "node dist/index.js",
    "build:watch": "tsc -w", 
    "start:watch": "nodemon --watch dist --delay 200ms --exec npm start"
  },
  "keywords": [
    "Node",
    "NodeJs",
    "TypeScript",
    "JavaScript",
    "TS",
    "JS"
  ],
  "author": "Anatoliy Timoshenko",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.3",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "concurrently": "^8.2.2",
    "typescript": "^5.4.2",
    "@types/node": "^20.11.30"
  }
}
